{
	"name": "hound-lite",
	"version": "1.0.0",
	"description": "AJAX Search Plugin",
	"main": "index.js",
	"directories": {
		"test": "tests"
	},
	"scripts": {
		"watch": "cls && webpack --watch --mode development",
		"compile": "cls && webpack --progress --mode production",
		"translate": "wp i18n make-pot ./ languages/hound-lite.pot --exclude=builds/,node_modules/,src,builds --ignore-domain",
		"build": "cls && grunt preBuildClean && npm run compile && npm run translate && grunt build:pro && grunt finish"
	},
	"author": "Arafat Jamil",
	"license": "GPL-3.0",
	"dependencies": {
		"@wordpress/api-fetch": "^6.43.0",
		"@wordpress/element": "^5.23.0"
	},
	"devDependencies": {
		"@babel/core": "^7.17.8",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"@tailwindcss/postcss": "^4.1.17",
		"@wordpress/scripts": "^26.17.0",
		"autoprefixer": "^10.4.22",
		"babel-loader": "^8.2.4",
		"clean-webpack-plugin": "^4.0.0",
		"css-loader": "^6.7.1",
		"eslint-plugin-react": "^7.29.4",
		"grunt": "^1.0.4",
		"grunt-checktextdomain": "^1.0.1",
		"grunt-contrib-clean": "^2.0.0",
		"grunt-contrib-compress": "^1.6.0",
		"grunt-contrib-copy": "^1.0.0",
		"grunt-contrib-rename": "^0.2.0",
		"grunt-phpunit": "^0.3.6",
		"grunt-replace": "^1.0.1",
		"grunt-search": "^0.1.8",
		"grunt-wp-i18n": "^1.0.3",
		"gulp": "4.0.2",
		"load-grunt-tasks": "^5.1.0",
		"mini-css-extract-plugin": "^2.6.0",
		"postcss": "^8.5.6",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"style-loader": "^3.3.1",
		"tailwindcss": "^4.1.17",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	}
}
