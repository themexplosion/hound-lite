
{
  "name": "react-webpack-grunt-dashboard",
  "version": "1.2.0",
  "private": true,
  "description": "WordPress plugin admin dashboard powered by React, Webpack, Grunt, TailwindCSS v4",
  "scripts": {
    "dev": "grunt dev",
    "hmr": "grunt hmr",
