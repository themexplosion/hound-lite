    "@babel/preset-react": "^7.28.5",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "mini-css-extract-plugin": "^2.9.4",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "@tailwindcss/postcss": "^4.1.13",
    "tailwindcss": "^4.1.17",
    "grunt": "^1.6.1",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-watch": "^1.1.0",
